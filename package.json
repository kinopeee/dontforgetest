{
  "name": "dontforgetest",
  "publisher": "kinopeee",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.0.113",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Testing",
    "Machine Learning"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dontforgetest",
          "title": "%viewsContainers.activitybar.title%",
          "icon": "media/dontforgetest-view.svg"
        }
      ]
    },
    "views": {
      "dontforgetest": [
        {
          "id": "dontforgetest.controlPanel",
          "name": "%views.controlPanel.name%",
          "type": "webview"
        },
        {
          "id": "dontforgetest.progressView",
          "name": "%views.progressView.name%",
          "type": "tree"
        },
        {
          "id": "dontforgetest.outputView",
          "name": "%views.outputView.name%",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "dontforgetest.generateTest",
        "title": "%command.generateTest.title%"
      },
      {
        "command": "dontforgetest.openPanel",
        "title": "%command.openPanel.title%"
      },
      {
        "command": "dontforgetest.generateTestFromCommit",
        "title": "%command.generateTestFromCommit.title%"
      },
      {
        "command": "dontforgetest.generateTestFromCommitRange",
        "title": "%command.generateTestFromCommitRange.title%"
      },
      {
        "command": "dontforgetest.generateTestFromWorkingTree",
        "title": "%command.generateTestFromWorkingTree.title%"
      },
      {
        "command": "dontforgetest.showTestGeneratorOutput",
        "title": "%command.showTestGeneratorOutput.title%"
      },
      {
        "command": "dontforgetest.selectDefaultModel",
        "title": "%command.selectDefaultModel.title%"
      },
      {
        "command": "dontforgetest.openSettings",
        "title": "%command.openSettings.title%",
        "icon": "$(gear)"
      },
      {
        "command": "dontforgetest.openLatestPerspective",
        "title": "%command.openLatestPerspective.title%"
      },
      {
        "command": "dontforgetest.openLatestExecutionReport",
        "title": "%command.openLatestExecutionReport.title%"
      },
      {
        "command": "dontforgetest.openLatestMergeInstruction",
        "title": "%command.openLatestMergeInstruction.title%"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dontforgetest.openSettings",
          "when": "view == dontforgetest.controlPanel",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "dontforgetest.cursorAgentPath": {
          "type": "string",
          "default": "",
          "description": "%configuration.cursorAgentPath.description%"
        },
        "dontforgetest.maxParallelTasks": {
          "type": "number",
          "default": 4,
          "description": "%configuration.maxParallelTasks.description%"
        },
        "dontforgetest.defaultModel": {
          "type": "string",
          "default": "",
          "description": "%configuration.defaultModel.description%"
        },
        "dontforgetest.customModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "%configuration.customModels.description%"
        },
        "dontforgetest.testStrategyPath": {
          "type": "string",
          "default": "",
          "description": "%configuration.testStrategyPath.description%"
        },
        "dontforgetest.includeTestPerspectiveTable": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.includeTestPerspectiveTable.description%"
        },
        "dontforgetest.perspectiveReportDir": {
          "type": "string",
          "default": "docs/test-perspectives",
          "description": "%configuration.perspectiveReportDir.description%"
        },
        "dontforgetest.perspectiveGenerationTimeoutMs": {
          "type": "number",
          "default": 300000,
          "minimum": 0,
          "description": "%configuration.perspectiveGenerationTimeoutMs.description%"
        },
        "dontforgetest.testExecutionReportDir": {
          "type": "string",
          "default": "docs/test-execution-reports",
          "description": "%configuration.testExecutionReportDir.description%"
        },
        "dontforgetest.testCommand": {
          "type": "string",
          "default": "npm test",
          "description": "%configuration.testCommand.description%"
        },
        "dontforgetest.testExecutionRunner": {
          "type": "string",
          "enum": [
            "cursorAgent",
            "extension"
          ],
          "default": "extension",
          "description": "%configuration.testExecutionRunner.description%"
        },
        "dontforgetest.allowUnsafeTestCommand": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.allowUnsafeTestCommand.description%"
        },
        "dontforgetest.cursorAgentForceForTestExecution": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.cursorAgentForceForTestExecution.description%"
        },
        "dontforgetest.enablePreTestCheck": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.enablePreTestCheck.description%"
        },
        "dontforgetest.preTestCheckCommand": {
          "type": "string",
          "default": "npm run compile",
          "description": "%configuration.preTestCheckCommand.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src/",
    "test": "node ./out/test/runTest.js",
    "coverage": "c8 -r text -r lcov npm test",
    "test:runTest": "node ./node_modules/mocha/bin/mocha --ui tdd ./out/test/suite/runTest.test.js",
    "pretest:runTest": "npm run compile",
    "coverage:runTest": "c8 -r text -r lcov npm run test:runTest",
    "test:ja": "DONTFORGETEST_VSCODE_TEST_LOCALE=ja npm test",
    "test:en": "DONTFORGETEST_VSCODE_TEST_LOCALE=en npm test",
    "package:vsix": "npm run compile && node ./scripts/packageVsix.cjs",
    "vsix:install": "code --install-extension \"$(ls -t dontforgetest-*.vsix | head -1)\" --force",
    "vsix:build": "npm run package:vsix",
    "vsix:build-install": "npm run package:vsix && npm run vsix:install",
    "vsix:build-install:bump": "node ./scripts/vsixBuildInstallBumpCommit.cjs",
    "vsix:build:bump": "npm version patch --no-git-tag-version && npm run vsix:build"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.10.5",
    "@types/vscode": "^1.85.0",
    "@vscode/test-electron": "^2.4.1",
    "c8": "^10.1.3",
    "esbuild": "^0.27.2",
    "eslint": "^9.17.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1"
  },
  "keywords": [
    "test",
    "testing",
    "test-generation",
    "cursor",
    "ai"
  ],
  "author": "Kinopee",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/kinopeee/dontforgetest.git"
  },
  "bugs": {
    "url": "https://github.com/kinopeee/dontforgetest/issues"
  },
  "homepage": "https://github.com/kinopeee/dontforgetest#readme"
}
